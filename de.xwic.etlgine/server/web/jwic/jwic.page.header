<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- jWic Application '$context.applicationSetup.name' | http://www.jwic.de -->
<html>
<head>
	<title>#if($page.title)$page.title#else $page.name#end</title>
	<META http-equiv="Cache-Control" content="no-cache">
	<META http-equiv="Pragma" content="no-cache">
	<META http-equiv="Expires" content="-1">
	<META http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
	
	<script language="JavaScript">
		var _contextPath = "$contextPath";
		var _ajaxMode = $context.applicationSetup.useAjaxRendering;
	</script>
	
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic_browser.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/calendar/calendar_${page.getSessionContext().getLocale().getLanguage().toUpperCase()}.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic.js"></SCRIPT>
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/jwic.css">
